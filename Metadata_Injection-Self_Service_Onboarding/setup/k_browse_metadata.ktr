  <step>
    <name>Browse Metadata</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>pentaho_psql</connection>
    <sql>SELECT
  c.customer_name
, m.map_id
, m.file_type
, m.delimiter
, m.has_header
, md.field_name
, md."type"
, md."length"
, md.format
, md.currency
, md."decimal"
, md."grouping"
, md.map_field
, ftf.seq as tgt_seq
, ftf."type" as tgt_type
, ftf."length" as tgt_length
, ftf."precision" as tgt_precision
, ftf."trim_type" as tgt_trim_type
, ftf.format as tgt_format
, ftf.currency as tgt_currency
, ftf."decimal" as tgt_decimal
, ftf."grouping" as tgt_grouping
FROM map m left join map_detail md on m.map_id = md.map_id 
right join customer c on c.customer_name = m.customer_name 
left join file_type_fields ftf on md.map_field = ftf.field_name
where m.file_type = ftf.file_type


</sql>
